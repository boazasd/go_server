package templates

import "bez/bez_server/internal/models"

templ Users(users []models.User) {
  
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
for _, user := range users {
        <tr>
            <td>{ user.FirstName }</td>
            <td>{ user.LastName }</td>
            <td>{ user.Email }</td>
        </tr>
}
    </table>
}